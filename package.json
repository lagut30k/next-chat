{
  "name": "chat-next",
  "devDependencies": {
    "turbo": "^2.6.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "db:push": "turbo -F @chat-next/database push --ui=tui",
    "db:studio": "turbo -F @chat-next/database studio"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "npm@11.6.2"
}
